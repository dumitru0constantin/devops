provider "aws" {
  region = var.region
}

module "security_group" {
  source = "./security_group"
}

module "ec2_instance" {
  source = "./ec2_instance"
  instance_type = var.instance_type
  ami_id        = var.ami_id
  key_name      = var.key_name
  security_group_id = module.security_group.security_group_id
  create_instance = var.create_instance
}

